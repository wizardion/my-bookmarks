{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0.1",
	"name": "My-Bookmarks",
	"description": "My-Bookmarks",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"bookmarks",
		"storage"
	],
	"host_permissions": [
		"https://*/",
		"http://*/"
	],
	"action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		},
		"default_title": "Bookmarks Checker"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
